                                        <li><a href="#"><img src="assets/img/user-7.jpg" alt="" /></a></li>
                                        <li><a href="#"><img src="assets/img/user-8.jpg" alt="" /></a></li>
                                        <li><a href="#"><img src="assets/img/user-9.jpg" alt="" /></a></li>
                                    </ul>
                                </div>
                                <!-- end section-container -->
                            </div>
                            <!-- end col-4 -->
                        </div>
                        <!-- end row -->
                    </div>
                    <!-- end container -->
                </div>
                <!-- end #footer -->
            {% endblock %}

    </body>
</html>
